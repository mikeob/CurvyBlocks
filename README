<script type="text/javascript">
   function check_clars() {
    new Ajax.PeriodicalUpdater('clarbox','script/check_for_new_clars.php',{frequency:120});
  }

  function delayed_check_clars(seconds) {
    if (seconds > 0) {
      setTimeout(check_clars, seconds*1000);
    } else {
      check_clars();
    }
  }
</script>
<script type="text/javascript">

    function countdown(timeval) {

    //var timeval = 10;
    function twodigit(x) {
      // There must be a better way in Javascript?
      if (x < 10) return "0" + x;
      return x;
    }

    function formatTime(timeRem) {
      if(timeRem == 0) {
	document.getElementById("countdown").innerHTML = "Contest is starting";
      } else {
	document.getElementById("countdown").innerHTML = "Contest is starting in " + twodigit(Math.floor(timeRem / 60)) + ":" + twodigit((timeRem % 60));
      }
    }

    function updateTime() {
      formatTime(--timeval);
      if(timeval > 0) {
	setTimeout(updateTime, 1000);
      }
    }
    updateTime();
  }
</script>

